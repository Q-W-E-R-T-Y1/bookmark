{
  "version": 2,
  "builds": [
    {
      "src": "api/index.cjs",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs20.x"
      }
    },
    {
      "src": "./package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}
